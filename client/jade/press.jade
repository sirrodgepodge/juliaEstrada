#press
  .table
    each quoteRow in quotes
      .table-row
        each quote in quoteRow
          section.press-sect
            img(src=quote.image).press-icon
            |         
            p.quote
              | #[i.icomoon.fa-quote-left] 
              each line in quote.content
                |  #{line} #[br]
            span.author -#{quote.author}
